function calculateChange(payment, cost) {
  // 여기에 코드를 작성하세요
  let a,b,c,d;
  let e = payment-cost
  a=e/50000
  b=(e%50000)/10000
  c=(e%50000)%10000/5000
  d=(e%50000)%10000%5000/1000
  console.log(`50000원 지폐: ${Math.floor(a)}장`)
  console.log(`10000원 지폐: ${Math.floor(b)}장`)
  console.log(`5000원 지폐: ${Math.floor(c)}장`)
  console.log(`1000원 지폐: ${Math.floor(d)}장`)


}

// 테스트 코드
calculateChange(100000, 33000);
console.log('');
calculateChange(500000, 378000);